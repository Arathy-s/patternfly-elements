{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Textinput",
  "description": "A web component text input",
  "type": "object",
  "tag": "pfe-textinput",
  "class": "PfeTextinput",
  "category": "content",
  "properties": {
    "slots": {
      "title": "Slots",
      "description": "Definition of the supported slots",
      "type": "object",
      "properties": {
        "default": {
          "title": "Default slot",
          "type": "array",
          "namedSlot": false,
          "items": {
            "oneOf": [
              {
                "$ref": "input"
              }
            ]
          }
        }
      },
      "required": ["default"]
    },
    "attributes": {
      "title": "Attributes",
      "type": "object",
      "properties": {}
    },
    "events": {
      "title": "Events",
      "description": "Definition of the supported events",
      "type": "object",
      "properties": {}
    }
  },
  "required": ["slots", "attributes", "events"],
  "additionalProperties": false
}
