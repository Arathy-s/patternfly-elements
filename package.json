{
  "name": "@patternfly/patternfly-elements",
  "scripts": {
    "start": "npm run doc-listing-inject && spandx",
    "lerna": "lerna",
    "test": "bash scripts/test.sh",
    "dev": "bash scripts/dev.sh",
    "build": "bash scripts/build.sh",
    "clean": "rm -rf node_modules package-lock.json {elements,themes}/*/{node_modules,package-lock.json}",
    "bootstrap": "lerna bootstrap --hoist",
    "postinstall": "npm run bootstrap",
    "test-suite-inject": "node scripts/test-suite-inject.js",
    "doc-listing-inject": "node scripts/doc-listing-inject.js",
    "license-update": "node scripts/license-update.js",
    "storybook": "start-storybook -c .storybook -p 9001 -s ./elements",
    "build-storybook": "build-storybook -c .storybook -o .storybook_out",
    "gh-pages-storybook": "storybook-to-ghpages",
    "deploy-storybook": "npm run build-storybook && npm run gh-pages-storybook",
    "new": "cd elements && yo --no-insight --no-update-notifier rhelement && npm run post-new",
    "post-new": "npm run test-suite-inject && npm run doc-listing-inject && npm run bootstrap"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test-suite-inject > /dev/null && lint-staged"
    }
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.2.2",
    "@babel/plugin-external-helpers": "7.2.0",
    "@babel/preset-env": "7.2.3",
    "@polymer/iron-test-helpers": "3.0.1",
    "@storybook/addon-actions": "3.4.8",
    "@storybook/addon-knobs": "3.4.8",
    "@storybook/addon-notes": "3.4.8",
    "@storybook/addon-options": "3.4.8",
    "@storybook/addon-viewport": "3.4.8",
    "@storybook/cli": "3.4.8",
    "@storybook/polymer": "3.4.8",
    "@storybook/storybook-deployer": "2.8.1",
    "@webcomponents/webcomponentsjs": "2.2.4",
    "browser-sync": "2.24.6",
    "chalk": "2.4.2",
    "clean-html": "1.5.0",
    "css-scss": "0.1.1",
    "decomment": "0.9.1",
    "del": "3.0.0",
    "dialog-polyfill": "0.4.10",
    "generator-rhelement": "0.6.4",
    "global": "4.3.2",
    "gulp": "4.0.0",
    "gulp-babel": "8.0.0",
    "gulp-banner": "0.1.3",
    "gulp-clean-css": "3.10.0",
    "gulp-rename": "1.4.0",
    "gulp-replace": "1.0.0",
    "gulp-sass": "4.0.1",
    "gulp-sequence": "1.0.0",
    "gulp-shell": "0.6.5",
    "gulp-strip-css-comments": "2.0.0",
    "gulp-svg-sprite": "1.4.0",
    "gulp-trim": "1.0.2",
    "gulp-uglify": "3.0.1",
    "husky": "1.3.1",
    "lerna": "3.10.6",
    "lint-staged": "7.2.0",
    "lodash": "4.17.10",
    "lodash.clonedeep": "4.5.0",
    "lorem-ipsum": "1.0.6",
    "node-sass": "4.11.0",
    "polymer-webpack-loader": "2.0.3",
    "prettier": "1.16.0",
    "requirejs": "2.3.5",
    "rollup": "0.66.6",
    "rollup-plugin-babel": "4.3.2",
    "rollup-plugin-commonjs": "9.1.5",
    "rollup-plugin-node-resolve": "3.3.0",
    "rollup-plugin-uglify": "4.0.0",
    "spandx": "1.0.3",
    "strip-css-comments": "3.0.0",
    "trim": "0.0.1",
    "wct-browser-legacy": "1.0.1",
    "web-component-tester": "6.9.2",
    "whatwg-fetch": "3.0.0",
    "yo": "1.8.5"
  },
  "dependencies": {
    "@babel/polyfill": "7.2.5",
    "gh-pages": "2.0.0"
  },
  "lint-staged": {
    "*.{js,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "private": true
}
